{
  "name": "@repo/ui",
  "version": "0.0.0",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    "./styles.css": "./dist/index.css",
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "pnpm run build:styles && pnpm run build:components",
    "build:styles": "pnpm dlx @tailwindcss/cli -i ./src/styles.css -o ./dist/index.css",
    "build:components": "tsc",
    "check-types": "tsc --noEmit",
    "dev": "concurrently \"pnpm run dev:styles\" \"pnpm run dev:components\"",
    "dev:styles": "pnpm dlx @tailwindcss/cli -i ./src/styles.css -o ./dist/index.css --watch",
    "dev:components": "tsc --watch",
    "lint": "eslint src --max-warnings 0"
  },
  "devDependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/css": "^11.13.5",
    "@emotion/styled": "^11.14.1",
    "@konomi-app/k2": "^3.4.1",
    "@konomi-app/ui-react": "^3.0.1",
    "@mui/material": "^7.3.6",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@repo/constants": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/tailwindcss": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@repo/utils": "workspace:*",
    "@turbo/gen": "^2.7.2",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "dompurify": "^3.3.1",
    "eslint": "^9.39.2",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-error-boundary": "^6.0.0",
    "react-resizable-panels": "^3",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tunnel-rat": "^0.1.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "latest"
  },
  "peerDependencies": {
    "@dnd-kit/core": "^6.0.0",
    "@dnd-kit/sortable": ">=9.0.0",
    "@dnd-kit/utilities": "^3.0.0",
    "@emotion/css": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@konomi-app/k2": ">=1.0.0",
    "@konomi-app/ui-react": "^2.0.0",
    "@mui/material": "^6.0.0 || ^7.0.0",
    "@radix-ui/react-accordion": "^1.0.0",
    "@radix-ui/react-avatar": "^1.0.0",
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-popover": "^1.0.0",
    "@radix-ui/react-scroll-area": "^1.0.0",
    "@radix-ui/react-slot": "^1.0.0",
    "@radix-ui/react-tooltip": "^1.0.0",
    "@repo/constants": "workspace:*",
    "@repo/tailwindcss": "workspace:*",
    "@repo/utils": "workspace:*",
    "class-variance-authority": ">=0.7.0",
    "clsx": "^2.0.0",
    "dompurify": "^3.0.0",
    "lucide-react": ">=0.300.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-error-boundary": ">=4.0.0",
    "react-resizable-panels": "^2.0.0 || ^3.0.0",
    "tailwind-merge": "^2.0.0 || ^3.0.0",
    "tunnel-rat": ">=0.1.0",
    "tw-animate-css": "^1.0.0"
  }
}
