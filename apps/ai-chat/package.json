    "@heroicons/react": "^2.2.0",
    "@konomi-app/ketch": "^0.4.0",
    "@konomi-app/kintone-utilities": "^6.1.0",
    "@konomi-app/kintone-utilities-jotai": "^2.0.1",
    "@konomi-app/kintone-utilities-react": "^3.0.2",
    "@konomi-app/ui": "^3.0.0",
    "@konomi-app/ui-react": "^3.0.1",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
