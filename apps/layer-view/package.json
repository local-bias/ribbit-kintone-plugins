    "nanoid": "^5.1.5",
    "notistack": "^3.0.2",
    "react": "workspace:*",
    "react-dom": "workspace:*",
    "react-error-boundary": "^6.0.0",
    "remeda": "^2.30.0",
    "tiny-invariant": "^1.3.3",
    "zod": "^4.1.5"
