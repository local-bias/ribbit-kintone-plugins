    "@repo/tailwindcss": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@tailwindcss/cli": "^4.1.12",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.3.0",
    "@types/react": "workspace:*",
