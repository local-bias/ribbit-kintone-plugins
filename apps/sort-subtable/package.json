{
  "name": "sort-subtable",
  "version": "1.3.0",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "init": "plugin init && plugin genkey",
    "build:css-desktop": "pnpm dlx @tailwindcss/cli -i ./src/styles/desktop.css -o ./.plugin/contents/desktop.css --minify",
    "build:css-config": "pnpm dlx @tailwindcss/cli -i ./src/styles/config.css -o ./.plugin/contents/config.css --minify",
    "build:js": "plugin build",
    "build": "concurrently \"pnpm:build:*\" && plugin zip",
    "standalone": "pnpm run build && plugin zip --env standalone",
    "dev:js": "plugin dev --cert-dir C:/workspaces/apps/cert",
    "dev:css-desktop": "pnpm dlx @tailwindcss/cli -i ./src/styles/desktop.css -o ./.plugin/dev/desktop.css --watch",
    "dev:css-config": "pnpm dlx @tailwindcss/cli -i ./src/styles/config.css -o ./.plugin/dev/config.css --watch",
    "dev": "concurrently \"pnpm:dev:*\""
  },
  "browser": {
    "path": false
  },
  "devDependencies": {
    "@emotion/css": "^11.13.0",
    "@emotion/react": "11.13.0",
    "@emotion/styled": "11.13.0",
    "@kintone/dts-gen": "8.0.12",
    "@kintone/rest-api-client": "5.5.2",
    "@konomi-app/k2": "^3.0.8",
    "@konomi-app/kbuild": "^3.1.2",
    "@konomi-app/kintone-utilities": "^5.17.1",
    "@konomi-app/kintone-utility-component": "^1.3.1",
    "@konomi-app/prettier-config": "^1.1.2",
    "@konomi-app/ui": "^3.0.0",
    "@konomi-app/ui-react": "^3.0.1",
    "@lb-ribbit/kintone-secrets": "0.3.0",
    "@lb-ribbit/kintone-xapp": "0.4.0",
    "@mui/icons-material": "5.16.6",
    "@mui/material": "5.16.6",
    "@types/luxon": "3.4.2",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "concurrently": "^8.2.2",
    "dotenv": "16.4.5",
    "immer": "10.1.1",
    "luxon": "3.4.4",
    "notistack": "3.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "4.0.13",
    "react-feather": "^2.0.10",
    "recoil": "0.7.7",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.5.4"
  }
}
