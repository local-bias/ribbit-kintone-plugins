{
  "name": "my-theme",
  "version": "0.1.0",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "init": "plugin init && plugin genkey",
    "build:css-desktop": "pnpm dlx @tailwindcss/cli -i ./src/styles/desktop.css -o ./.plugin/contents/desktop.css --minify",
    "build:css-config": "pnpm dlx @tailwindcss/cli -i ./src/styles/config.css -o ./.plugin/contents/config.css --minify",
    "build:js": "plugin build",
    "build": "concurrently \"pnpm:build:*\" && plugin zip",
    "standalone": "pnpm run build && plugin zip --env standalone",
    "dev:js": "plugin dev",
    "dev:css-desktop": "pnpm dlx @tailwindcss/cli -i ./src/styles/desktop.css -o ./.plugin/dev/desktop.css --watch",
    "dev:css-config": "pnpm dlx @tailwindcss/cli -i ./src/styles/config.css -o ./.plugin/dev/config.css --watch",
    "dev": "concurrently \"pnpm:dev:*\""
  },
  "browser": {
    "path": false
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@konomi-app/kintone-utilities": "^5.13.0",
    "@konomi-app/kintone-utilities-jotai": "^1.2.3",
    "@konomi-app/kintone-utilities-react": "^2.1.0",
    "@konomi-app/ui": "^2.3.0",
    "@konomi-app/ui-react": "^2.0.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-context-menu": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-slot": "^1.2.3",
    "@repo/constants": "workspace:*",
    "@repo/jotai": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/ui": "workspace:*",
    "@repo/utils": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "immer": "^10.1.1",
    "jotai": "^2.12.5",
    "jotai-derive": "^0.1.3",
    "jotai-optics": "^0.4.0",
    "lucide-react": "^0.514.0",
    "luxon": "^3.6.1",
    "nanoid": "^5.1.5",
    "notistack": "^3.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "remeda": "^2.23.0",
    "tiny-invariant": "^1.3.3",
    "zod": "^3.25.63"
  },
  "devDependencies": {
    "@kintone/dts-gen": "^8.1.2",
    "@konomi-app/k2": "^1.10.0",
    "@repo/eslint-config": "workspace:*",
    "@repo/tailwindcss": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@tailwindcss/cli": "^4.1.10",
    "@types/luxon": "^3.6.2",
    "@types/node": "^24.0.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@uiw/react-json-view": "2.0.0-alpha.32",
    "concurrently": "^9.1.2",
    "dotenv": "^16.5.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.2.0",
    "tailwindcss": "^4.1.10",
    "ts-loader": "^9.5.2",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.8.3"
  }
}