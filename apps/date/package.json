    "@konomi-app/kintone-utilities-react": "^2.1.0",
    "@konomi-app/ui": "^2.3.0",
    "@konomi-app/ui-react": "^2.0.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@radix-ui/react-alert-dialog": "^1.1.13",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-slot": "^1.2.2",
    "@repo/constants": "workspace:*",
    "@repo/jotai": "workspace:*",
    "@repo/ui": "workspace:*",
    "@repo/utils": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "immer": "^10.1.1",
    "jotai": "^2.12.4",
    "jotai-derive": "^0.1.3",
    "jotai-eager": "^0.2.1",
    "lucide-react": "^0.511.0",
    "luxon": "^3.6.1",
    "nanoid": "^5.1.5",
    "notistack": "^3.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "recoil": "^0.7.7",
    "remeda": "^2.21.8",
    "sweetalert2": "^11.21.2",
    "tailwind-merge": "^3.3.0",
