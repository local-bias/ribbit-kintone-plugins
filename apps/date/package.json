    "@repo/eslint-config": "workspace:*",
    "@repo/tailwindcss": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.15.19",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "concurrently": "^9.1.2",
    "dotenv": "^16.5.0",
    "tailwindcss": "^4.1.7",
    "ts-loader": "^9.5.2",
    "tw-animate-css": "^1.3.0",
    "typescript": "^5.8.3"
  }
