    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^9.2.1",
    "dotenv": "17.2.3",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "tailwind-merge": "^3.4.0",
