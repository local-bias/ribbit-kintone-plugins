{
  "name": "editable",
  "version": "1.2.0",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "init": "plugin init && plugin genkey",
    "build:css-desktop": "pnpm dlx @tailwindcss/cli -i ./src/styles/desktop.css -o ./.plugin/contents/desktop.css --minify",
    "build:css-config": "pnpm dlx @tailwindcss/cli -i ./src/styles/config.css -o ./.plugin/contents/config.css --minify",
    "build:js": "plugin build",
    "build": "concurrently \"pnpm:build:*\" && plugin zip",
    "standalone": "concurrently \"pnpm:build:*\" && plugin zip --env standalone",
    "dev:js": "plugin dev",
    "dev:css-desktop": "pnpm dlx @tailwindcss/cli -i ./src/styles/desktop.css -o ./.plugin/dev/desktop.css --watch",
    "dev:css-config": "pnpm dlx @tailwindcss/cli -i ./src/styles/config.css -o ./.plugin/dev/config.css --watch",
    "dev": "concurrently \"pnpm:dev:*\""
  },
  "browser": {
    "path": false
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@kintone/rest-api-client": "6.1.0",
    "@konomi-app/kintone-utilities": "^5.17.0",
    "@konomi-app/kintone-utilities-jotai": "^2.0.1",
    "@konomi-app/kintone-utilities-react": "^3.0.0",
    "@konomi-app/kintone-utility-component": "^1.3.1",
    "@konomi-app/ui-react": "^3.0.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@repo/constants": "workspace:*",
    "@repo/jotai": "workspace:*",
    "@repo/tailwindcss": "workspace:*",
    "@repo/ui": "workspace:*",
    "@repo/utils": "workspace:*",
    "immer": "11.1.0",
    "jotai": "^2.16.0",
    "jotai-derive": "^0.1.3",
    "luxon": "3.7.2",
    "nanoid": "^5.1.6",
    "notistack": "3.0.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-error-boundary": "6.0.0",
    "remeda": "^2.32.0",
    "tiny-invariant": "^1.3.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@kintone/dts-gen": "9.0.0",
    "@konomi-app/k2": "^2.1.5",
    "@repo/eslint-config": "workspace:*",
    "@repo/k2": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@tailwindcss/cli": "^4.1.18",
    "@types/luxon": "3.7.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^9.2.1",
    "tailwindcss": "^4.1.18",
    "ts-loader": "^9.5.4",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.9.3",
    "utility-types": "^3.11.0"
  }
}
