    "@repo/typescript-config": "workspace:*",
    "@types/dompurify": "^3.2.0",
    "@types/luxon": "3.6.2",
    "@types/node": "^24.0.13",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "concurrently": "^9.2.0",
    "dotenv": "17.0.0",
    "eslint-plugin-react": "^7.37.5",
