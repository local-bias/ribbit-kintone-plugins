{
  "name": "smart-view",
  "version": "1.0.0",
  "scripts": {
    "init": "plugin init && plugin genkey",
    "build:css-desktop": "pnpm dlx @tailwindcss/cli -i ./src/styles/desktop.css -o ./.plugin/contents/desktop.css --minify",
    "build:css-config": "pnpm dlx @tailwindcss/cli -i ./src/styles/config.css -o ./.plugin/contents/config.css --minify",
    "build:js": "plugin build",
    "build": "concurrently \"pnpm:build:*\" && plugin zip --ppk ./.plugin/smart-view.ppk",
    "standalone": "concurrently \"pnpm:build:*\" && plugin zip --env standalone --ppk ./.plugin/smart-view.ppk",
    "dev:js": "plugin dev --ppk /home/dev/config/kintone-plugin-dev.ppk --cert-dir /home/dev/config/cert",
    "dev:css-desktop": "pnpm dlx @tailwindcss/cli -i ./src/styles/desktop.css -o ./.plugin/dev/desktop.css --watch",
    "dev:css-config": "pnpm dlx @tailwindcss/cli -i ./src/styles/config.css -o ./.plugin/dev/config.css --watch",
    "dev": "concurrently \"pnpm:dev:*\""
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@konomi-app/kintone-utilities": "^6.1.0",
    "@konomi-app/kintone-utilities-jotai": "^2.0.1",
    "@konomi-app/kintone-utilities-react": "^3.0.2",
    "@konomi-app/ui-react": "3.0.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "clsx": "^2.1.1",
    "dompurify": "^3.3.1",
    "immer": "^11.1.0",
    "jotai": "^2.16.0",
    "jotai-eager": "^0.2.3",
    "jotai-effect": "^2.2.1",
    "jotai-optics": "^0.4.0",
    "lucide-react": "^0.562.0",
    "luxon": "^3.7.2",
    "nanoid": "^5.1.6",
    "notistack": "^3.0.2",
    "optics-ts": "^2.4.1",
    "papaparse": "^5.5.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-error-boundary": "^6.0.0",
    "remeda": "^2.32.0",
    "tailwind-merge": "^3.4.0",
    "tiny-invariant": "^1.3.3",
    "utility-types": "^3.11.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@kintone/dts-gen": "^9.0.0",
    "@konomi-app/k2": "^3.4.1",
    "@types/dompurify": "^3.2.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^25.0.3",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "tailwindcss": "^4.1.18",
    "ts-loader": "^9.5.4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  }
}
